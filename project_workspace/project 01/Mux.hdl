/** 
 * Multiplexor.  If sel=0 then out = a else out = b.
 */

CHIP Mux {

    IN  a, b, sel;
    OUT out;

    PARTS:
    Not(in=a, out=nota);
    And(a=sel, b=b, out=andSelB);
    And(a=nota, b=andSelB, out=out);
}
